{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": [ "ES6", "ES2015", "DOM" ],
    "noEmitOnError": true,
    "importHelpers": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "moduleResolution": "Node"
  }
}
